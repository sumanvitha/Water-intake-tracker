<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">

    <TextView
        android:id="@+id/target"
        android:layout_width="286dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="18dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar2"
        android:layout_marginEnd="36dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="36dp"
        app:layout_constraintVertical_bias="0.876" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="277dp"
        android:layout_height="46dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="217dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.363" />
    <ImageButton
        android:id="@+id/graph"
        android:layout_width="52dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/graph"
        android:layout_marginStart="62dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/log"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/log"
        android:layout_marginLeft="62dp" />

    <ImageButton
        android:id="@+id/log"
        android:layout_width="48dp"
        android:layout_height="43dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/list"
        android:layout_marginStart="41dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="39dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/graph"
        android:layout_marginLeft="41dp" />

    <ImageButton
        android:id="@+id/settings"
        android:layout_width="46dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/settings"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/log"
        android:layout_marginEnd="33dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/log"
        app:layout_constraintLeft_toRightOf="@+id/log"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="33dp"
        app:layout_constraintHorizontal_bias="0.294"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/glass"
        android:layout_width="55dp"
        android:layout_height="0dp"
        android:layout_marginStart="109dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@android:color/transparent"
        app:srcCompat="@drawable/water"
        android:scaleType="fitXY"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/bottle"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bottle"
        android:layout_marginLeft="109dp" />

    <ImageButton
        android:id="@+id/bottle"
        android:layout_width="92dp"
        android:layout_height="78dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/bottle"
        android:layout_marginStart="99dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="99dp"
        app:layout_constraintRight_toRightOf="@+id/delete"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="53dp"
        app:layout_constraintLeft_toRightOf="@+id/cup"
        android:layout_marginLeft="99dp"
        android:layout_marginRight="99dp"
        android:scaleType="fitXY"/>

    <ImageButton
        android:id="@+id/cup"
        android:layout_width="62dp"
        android:layout_height="82dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/cup100"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/glass"
        android:layout_marginLeft="16dp"
        android:scaleType="fitXY" />


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/addWater"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/bottle"
        android:layout_marginEnd="24dp"
        app:layout_constraintRight_toRightOf="@+id/delete"
        android:layout_marginTop="6dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="6dp"
        app:layout_constraintTop_toTopOf="@+id/bottle"
        app:layout_constraintLeft_toRightOf="@+id/bottle"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="24dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/delete"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="72dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="77dp"
        android:clickable="true"
        android:tint="@android:color/transparent"
        app:backgroundTint="@color/colorAccent"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="@+id/addWater"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar2"
        app:rippleColor="@android:color/transparent"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintVertical_bias="0.473" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100ml"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="9dp"
        app:layout_constraintRight_toRightOf="@+id/cup"
        app:layout_constraintTop_toBottomOf="@+id/cup"
        android:layout_marginRight="9dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="250ml"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="5dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/glass"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/glass"
        android:layout_marginLeft="5dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="500ml"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="26dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/bottle"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/bottle"
        android:layout_marginLeft="26dp" />

</android.support.constraint.ConstraintLayout>